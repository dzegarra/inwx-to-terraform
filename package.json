{
  "name": "inwx-to-terraform",
  "version": "1.2.0",
  "description": "Tool to generate terraform resources and import blocks to start managing your domains with the official Terraform INWX provider",
  "main": "index.mjs",
  "scripts": {
    "start": "node index.mjs",
    "fmt": "terraform fmt -recursive"
  },
  "bin": {
    "inwx-to-terraform": "./index.mjs"
  },
  "type": "module",
  "keywords": [
    "inwx",
    "terraform",
    "import"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Daniel Zegarra <dzegarra@gmail.com>",
  "license": "MIT",
  "repository": "github:dzegarra/inwx-to-terraform",
  "dependencies": {
    "domrobot-client": "^3.1.0",
    "dotenv": "^16.3.1"
  }
}
